{
  "name": "@paulkinlan/claude-breadboard-kit",
  "version": "3.0.0",
  "description": "A Kit that allows you to build Breadboard graphs for Claude.",
  "main": "./dist/src/index.js",
  "exports": "./dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "type": "module",
  "files": [
    "dist/src"
  ],
  "scripts": {
    "test": "FORCE_COLOR=1 ava",
    "build": "FORCE_COLOR=1 tsc --b",
    "watch": "FORCE_COLOR=1 tsc --b --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulkinlan/claude-breadboard-kit.git"
  },
  "ava": {
    "files": [
      "tests/**/*.ts"
    ],
    "workerThreads": false,
    "typescript": {
      "rewritePaths": {
        "./": "dist/"
      },
      "compile": false
    }
  },
  "author": "Paul Kinlan",
  "license": "Apache-2.0",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@google-labs/core-kit": "^0.2.2",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "ava": "^5.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.1",
    "@anthropic-ai/tokenizer": "^0.0.4",
    "@google-labs/breadboard": "^0.10.1",
    "js-tiktoken": "^1.0.8"
  }
}
